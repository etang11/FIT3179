{
    "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
    "width": 200,
    "height": 400,
    "title": "airbnb property price + availability",
    "data": {
        "url": "https://raw.githubusercontent.com/etang11/FIT3179/master/AB_NYC_2019.csv"
    },
    
    "mark": {"type": "boxplot","clip":true},
    "encoding": {
    "x": {"field": "neighbourhood_group", "type": "nominal"},
    "color": {"field": "neighbourhood_group", "type": "nominal", "legend": null},
    "y": {
      "field": "price",
      "type": "quantitative",
      "scale": {"domainMax":300}
    }
  }


}